{{ form().openTag(csvForm)|raw }}
    <legend>Wgraj nowych użytkowników</legend>
    <div class="form-group">
        {{ formLabel(csvForm.get('csvFile')) }}
        {{ formElement(csvForm.get('csvFile')) }}
        {{ formElementErrors()
            .setMessageOpenFormat('<div class="text-danger pt-1">')
            .setMessageSeparatorString('<br />')
            .setMessageCloseString('</div>')
            .render(csvForm.get('csvFile'))
        |raw }}
    </div>
    <div class="form-group mt-2">
        {{ formElement(csvForm.get('submit')) }}
    </div>

{{ form().closeTag(csvForm)|raw }}
